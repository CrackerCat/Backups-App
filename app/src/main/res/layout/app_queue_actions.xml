<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/app_queue_search_button"
        style="@style/FloatingActionButtonInactive"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/app_queue_search_label"
        style="@style/FloatingActionButtonLabel"
        android:layout_marginEnd="16dp"
        android:text="@string/search_label_text"
        android:textColor="#212121"
        app:layout_constraintBottom_toBottomOf="@+id/app_queue_search_button"
        app:layout_constraintEnd_toStartOf="@+id/app_queue_search_button"
        app:layout_constraintTop_toTopOf="@+id/app_queue_search_button"
        tools:visibility="visible" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/app_queue_backup_button"
        style="@style/FloatingActionButtonInactive"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/app_queue_search_button"
        app:layout_constraintEnd_toEndOf="@+id/app_queue_search_button"
        app:layout_constraintStart_toStartOf="@+id/app_queue_search_button"
        tools:visibility="visible" />


    <TextView
        android:id="@+id/app_queue_backup_label"
        style="@style/FloatingActionButtonLabel"
        android:layout_marginEnd="16dp"
        android:text="@string/backup_label_text"
        android:textColor="#212121"
        app:layout_constraintBottom_toBottomOf="@+id/app_queue_backup_button"
        app:layout_constraintEnd_toStartOf="@+id/app_queue_backup_button"
        app:layout_constraintTop_toTopOf="@+id/app_queue_backup_button"
        tools:visibility="visible" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/app_queue_item_selection_button"
        style="@style/FloatingActionButtonInactive"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/app_queue_backup_button"
        app:layout_constraintEnd_toEndOf="@+id/app_queue_backup_button"
        app:layout_constraintStart_toStartOf="@+id/app_queue_backup_button"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/app_queue_item_selection_label"
        style="@style/FloatingActionButtonLabel"
        android:layout_width="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="@string/app_queue_item_selection_label_text"
        android:textColor="#212121"
        app:layout_constraintBottom_toBottomOf="@+id/app_queue_item_selection_button"
        app:layout_constraintEnd_toStartOf="@+id/app_queue_item_selection_button"
        app:layout_constraintTop_toTopOf="@+id/app_queue_item_selection_button"
        tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>
